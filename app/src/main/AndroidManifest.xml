<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pa1.picaday">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_picaday"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_picaday_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Settingpage" />
        <activity android:name=".Mypage" />
        <activity android:name=".AddActivity_Fragment.AddActivity_monthly" />
        <activity android:name=".AddActivity_Fragment.AddActivity_weekly" />
        <activity android:name=".AddActivity_Fragment.AddActivity_daily" />
        <activity android:name=".MainActivity_Fragment.MainActivity_monthly" />
        <activity android:name=".MainActivity_Fragment.MainActivity_weekly" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>